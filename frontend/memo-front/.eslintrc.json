{
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "prettier",
      "plugin:prettier/recommended"
    ],
    "plugins": ["react", "prettier"],
    "env": {
      "browser": true,
      "jasmine": true,
      "jest": true
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      }
    },
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto"
            }

        ],
        
        "react/prop-types" : "off"
    }
 
  }